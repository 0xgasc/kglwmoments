{
  "name": "king-gizzard-archive",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "ajv": "^8.17.1",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "form-data": "^4.0.4",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.523.0",
    "mongoose": "^8.15.2",
    "multer": "^2.0.1",
    "node-fetch": "^3.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:mobile": "GENERATE_SOURCEMAP=false INLINE_RUNTIME_CHUNK=false react-scripts build",
    "build:analyze": "npm run build && npx bundlesize",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy:vercel": "npm run build:mobile && vercel --prod",
    "deploy:netlify": "npm run build:mobile && netlify deploy --prod --dir=build",
    "lighthouse": "lhci autorun",
    "serve": "serve -s build -l 3000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@lhci/cli": "^0.13.0",
    "autoprefixer": "^10.4.21",
    "bundlesize": "^0.18.1",
    "lighthouse": "^11.7.0",
    "postcss": "^8.5.6",
    "serve": "^14.2.1",
    "tailwindcss": "^4.1.10"
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "500 kB",
      "compression": "gzip"
    },
    {
      "path": "./build/static/css/*.css",
      "maxSize": "50 kB",
      "compression": "gzip"
    }
  ]
}
